#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_nunjucks@2.4.2@nunjucks/bin/precompile" "$@"
  ret=$?
else 
  node  "$basedir/../_nunjucks@2.4.2@nunjucks/bin/precompile" "$@"
  ret=$?
fi
exit $ret
